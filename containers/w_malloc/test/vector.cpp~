#include "stdio.h"
#include "vec.hpp"
#include <catch2/catch_test_macros.hpp>
#include <cstdio>

TEST_CASE("default constructor", "[vec]") {
  auto v = Vector<int>();
  REQUIRE(v.empty());
  REQUIRE(v.size() == 0);
}

TEST_CASE("size constructor") {
  auto v = Vector<int>(1);
  v.push_back(1);
  v.push_back(2);
  REQUIRE(!v.empty());
  REQUIRE(v.size() == 2);
  // for(std::size_t i = 0; i < v.size(); i++) {
  //   std::printf("%i\n", v[i]);
  // }
}
